{
	"name": "roblox-api-explorer",
	"displayName": "Roblox API Explorer",
	"description": "Allows you to explore the Roblox API right from your IDE",
	"repository": "https://github.com/evaera/vscode-roblox-api-explorer",
	"version": "1.2.0",
	"publisher": "evaera",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:roblox-api-explorer.open"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "roblox-api-explorer.open",
				"title": "Open Roblox API Explorer"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/node-fetch": "^2.5.5",
		"@types/vscode": "^1.43.0",
		"@types/xml2js": "^0.4.5",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-prettier": "^3.1.2",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"prettier": "^2.0.4",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.0"
	}
}
